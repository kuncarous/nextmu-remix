{
    "name": "nextmu-portal",
    "version": "0.0.1",
    "private": true,
    "sideEffects": false,
    "type": "module",
    "scripts": {
        "preinstall": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0",
        "build:proto": "cross-env PROTO_PATH=./app/proto tsx ./proto/proto-generator.ts",
        "prebuild": "cross-env PROTO_PATH=./app/proto tsx ./proto/proto-generator.ts",
        "build": "remix vite:build",
        "deploy": "wrangler pages deploy ./build/client",
        "dev": "remix vite:dev",
        "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
        "start": "wrangler pages dev ./build/client",
        "typecheck": "tsc",
        "typegen": "wrangler types"
    },
    "dependencies": {
        "@grpc/grpc-js": "^1.10.6",
